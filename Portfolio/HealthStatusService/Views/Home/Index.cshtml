@model IEnumerable<HealthCheck.StatusTableEntity>

<!DOCTYPE html>
<html>
<head>
    <title>Health Monitoring Data</title>
</head>
<body>
    <h1>Health Monitoring Data</h1>
    <p>Portfolio service down status in the last hour</p>
    <table>
        <thead>
            <tr>
                <th>RowKey</th>
                <th>Status</th>
                <!-- Add other columns as needed -->
            </tr>
        </thead>
        <tbody>
            @foreach (var item in Model)
            {
<tr>
    <td>@item.RowKey</td>
    <td>@item.Status</td>
    <!-- Add other columns as needed -->
</tr>            }
        </tbody>
    </table>

</body>
</html>
